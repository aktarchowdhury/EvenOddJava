package com.aktar.even;

public class Switch {

	public static void main(String[] args) {
	int age;
	age=3;
	switch(age){
	case 1:
		System.out.println("Can smile");
	break;
	case 2:
		System.out.println("Can crawl");
		break;
	case 3:
		System.out.println("Can walk");
		break;
		default:
			System.out.println("I have no idea");
			break;
	}

	}

}
